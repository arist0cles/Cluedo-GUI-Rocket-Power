package squares;

import java.awt.Color;
import core.Location;

public class DoorSquare extends Square {
	private String name=""; 
	private Location location = null;
	private final Color DOORCOLOR = new Color(0, 0, 255);
	
	public DoorSquare(String name, Location location) {
		super(name, location);
		setName();
	}
	
	public void setName(){
		this.name="Door";
	}
	
	public void draw(int x1, int y1){
		super.draw(x1, y1, DOORCOLOR);
	}
	
	public String getName(){
		return this.name;
	}

	public boolean equals(Square square){
		return super.equals(square);
	}
}
