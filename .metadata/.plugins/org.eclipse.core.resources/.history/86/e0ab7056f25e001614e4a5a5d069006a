package squares;

import java.awt.Color;

import core.Location;

public class HallwaySquare extends Square {
	String name="";
	Location location=null;
	private final Color HALLCOLOR = new Color(120, 80, 50);

	public HallwaySquare(String name, Location location) {
		super(name, location);
		setName();
	}
	
	public void setName(){
		this.name = "Hall";
	}
	
	public String getName(){
		return this.name;
	}
	
	@Override
	public void draw(int x1, int y1, Color c){
		super.draw(x1, y1, HALLCOLOR);
	}

	public boolean equals(Square square){
		return super.equals(square);
	}
	
}
